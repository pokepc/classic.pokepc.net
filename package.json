{
  "name": "supereffective-website",
  "version": "3.11.0",
  "private": true,
  "repository": "git@github.com:itsjavi/supereffective.git",
  "license": "MIT",
  "author": "Javi Aguilar https://itsjavi.com",
  "scripts": {
    "build": "pnpm typecheck && pnpm build:data && next build && next-sitemap",
    "build:data": "pnpm run build:data:pokemon && bun run src/lib/data-client/box-presets/join-presets.ts",
    "build:data:pokemon": "bun json:minify src/lib/data-client/pokemon/legacy-pokemon.json",
    "db-migrate": "pnpm prisma migrate deploy",
    "db-studio": "pnpm prisma studio",
    "dev": "rm -rf ./.next && PORT=3001 next dev",
    "format": "sort-package-json && prettier --cache --ignore-path '.prettierignore' --write ./",
    "json:minify": "bun run scripts/format-json-file.ts --minify",
    "json:unminify": "bun run scripts/format-json-file.ts",
    "lint": "pnpm typecheck && prettier --ignore-path '.prettierignore' --check ./",
    "lint-fix": "pnpm format",
    "prepare": "pnpm prisma generate && sort-package-json",
    "qa": "pnpm quality-checks",
    "quality-checks": "pnpm quality-checks-nobuild && pnpm build",
    "quality-checks-nobuild": "rm -rf .next && pnpm prisma generate && pnpm lint && pnpm test",
    "start": "next start",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.435.0",
    "nanoid": "^5.1.6",
    "next": "14.2.6",
    "next-auth": "^4.24.13",
    "next-sitemap": "^4.2.3",
    "prisma": "^5.22.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.1.0",
    "react-modal": "^3.16.3",
    "resend": "^4.8.0",
    "sass": "^1.93.3",
    "sort-package-json": "^2.15.1",
    "zod": "^3.25.76",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.14.0",
    "@swc/jest": "^0.2.39",
    "@types/bun": "^1.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.13",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/react-modal": "^3.16.3",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "pwa-asset-generator": "^6.4.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "nodemailer": "^6.9.12"
  },
  "packageManager": "pnpm@9.4.0",
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
